<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<parent>
		<groupId>com.boozallen.aissemble</groupId>
		<artifactId>license-example-projects</artifactId>
		<version>1.3.0-SNAPSHOT</version>
	</parent>

	<modelVersion>4.0.0</modelVersion>

	<artifactId>closed-source-examples</artifactId>
	<packaging>pom</packaging>

	<name>aiSSEMBLE License::Closed Source</name>
	<description>Provides examples of how to use this capability on closed source projects</description>

	<modules>
		<module>booz-allen-closed-source-java</module>
		<module>booz-allen-closed-source-python</module>
	</modules>

	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<groupId>org.codehaus.mojo</groupId>
					<artifactId>license-maven-plugin</artifactId>
					<configuration>
						<licenseName>closed-source-license</licenseName>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>
</project>